    "class Employee:\n",
    "    def __init__(self, name, surname, base_salary, experience):\n",
    "        self.name = name\n",
    "        self.surname = surname\n",
    "        self.base_salary = base_salary\n",
    "        self.experience = experience\n",
    "\n",
    "class Developer(Employee):\n",
    "    def __init__(self, name, surname, base_salary, experience):\n",
    "        super().__init__(name, surname, base_salary, experience)\n",
    "    def counted_salary(self):\n",
    "        if self.experience > 2:\n",
    "            return round(self.base_salary + 200)\n",
    "        elif self.experience > 5:\n",
    "            return round(self.base_salary * 1.2 + 500)\n",
    "        else:\n",
    "            return round(self.base_salary)\n",
    "\n",
    "class Designer(Employee):\n",
    "    def __init__(self, name, surname, base_salary, experience, efficiency_ratio):\n",
    "        super().__init__(name, surname, base_salary, experience)\n",
    "        self.efficiency_ratio = efficiency_ratio\n",
    "    def counted_salary(self):\n",
    "        if self.experience > 2:\n",
    "            return round(self.base_salary * self.efficiency_ratio + 200)\n",
    "        elif self.experience > 5:\n",
    "            return round(self.base_salary * self.efficiency_ratio * 1.2 + 500)\n",
    "        else:\n",
    "            return round(self.base_salary * self.efficiency_ratio)\n",
    "\n",
    "class Manager(Employee):\n",
    "    def __init__(self, name, surname, base_salary, experience, team):\n",
    "        super().__init__(name, surname, base_salary, experience)\n",
    "        self.team = team\n",
    "    def counted_salary(self):\n",
    "        if self.experience > 2:\n",
    "            base_salary = self.base_salary + 200\n",
    "        elif self.experience > 5:\n",
    "            base_salary = self.base_salary * 1.2 + 500\n",
    "        else:\n",
    "            base_salary = self.base_salary\n",
    "\n",
    "        if len(self.team) > 5:\n",
    "            base_salary += 200\n",
    "        elif len(self.team) > 10:\n",
    "            base_salary += 300\n",
    "\n",
    "        developers = [employee for employee in self.team if isinstance(employee, Developer)]\n",
    "        if len(developers) > len(self.team) / 2:\n",
    "            base_salary *= 1.1\n",
    "\n",
    "        return round(base_salary)\n",
    "\n",
